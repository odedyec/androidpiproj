<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="510dp"
        android:layout_height="138dp"
        android:layout_x="64dp"
        android:layout_y="96dp"
        android:text="@string/hello_world" />

    <Button
        android:id="@+id/button1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="118dp"
        android:layout_height="76dp"
        android:layout_x="717dp"
        android:layout_y="84dp"
        android:onClick="printHello"
        android:text="OK" />

    <ImageView
        android:id="@+id/steeringWheel"
        android:layout_width="162dp"
        android:layout_height="171dp"
        android:layout_x="414dp"
        android:layout_y="225dp"
        
        android:src="@drawable/sw" />

</AbsoluteLayout>